    "e2e:affected": "yarn nx affected --target=e2e",
    "lint": "yarn lint:affected",
    "lint:all": "yarn nx run-many --target=lint",
    "lint:affected": "yarn nx affected --target=lint",
    "format": "yarn prettier --write '**/*.{ts,js,tsx,jsx,cjs,mjs,css,json,md,mdx,html,yaml,yml}'",
    "install:husky": "is-ci || husky",
    "prepare": "yarn install:husky",
    "postinstall": "yarn prepare"
