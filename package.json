  "scripts": {
    "start": "yarn start:marketplace",
    "start:marketplace": "yarn nx serve marketplace",
    "build": "yarn nx run-many --target=build --prod",
    "build:marketplace": "yarn nx build marketplace --prod",
    "build:storybook": "yarn nx build-storybook storybook --prod",
    "storybook": "yarn nx storybook storybook",
    "test": "yarn test:affected",
    "test:all": "yarn nx run-many --target=test",
    "test:affected": "yarn nx affected --target=test",
    "e2e": "yarn e2e:affected",
